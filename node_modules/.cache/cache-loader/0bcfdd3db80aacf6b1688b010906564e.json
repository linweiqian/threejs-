efined\" ||\r\n    val == \"null\" ||\r\n    val == \"NULL\"\r\n  ) {\r\n    return true;\r\n  }\r\n  return false;\r\n}\r\n\r\n/**\r\n * JS获取地址栏参数的值\r\n * @param name 对应的参数\r\n * @param search location.search || location.hash\r\n * @returns {*} 如果有，则返回参数值，没有则返回null\r\n */\r\nexport function getUrlParam(name, search) {\r\n  search = search || window.location.search.substr(1) || window.location.hash.split(\"?\")[1];\r\n  if (!search) {\r\n    return null;\r\n  }\r\n  let reg = new RegExp(\"(^|&)\" + name + \"=([^&]*)(&|$)\");\r\n  let r = search.match(reg);\r\n  if (r != null) return unescape(r[2]);\r\n  return null;\r\n}"],"mappings":"AAAA;AACA;AACA;AACA;AACA;AACA;AACA,OAAO,SAASA,WAAT,CAAqBC,KAArB,EAA4BC,MAA5B,EAAoC;EACzC;EACA,IAAID,KAAK,IAAI,IAAT,IAAiBA,KAAK,IAAIE,SAA1B,IAAuCF,KAAK,IAAI,EAAhD,IAAsDA,KAAK,IAAI,WAAnE,EAAgF;IAC9E,OAAO,GAAP;EACD,CAJwC,CAKzC;;;EACA,IAAIC,MAAM,IAAI,IAAV,IAAkBA,MAAM,IAAI,EAAhC,EAAoC;IAClC,OAAOD,KAAP;EACD;;EACD,OAAOA,KAAK,CAACG,QAAN,GAAiBC,MAAjB,CAAwB,CAAxB,EAA2BH,MAA3B,CAAP;AACD;AAED;AACA;AACA;AACA;AACA;;AACA,OAAO,SAASI,MAAT,CAAgBC,GAAhB,EAAqB;EAC1B,IACEA,GAAG,IAAIJ,SAAP,IACAI,GAAG,IAAI,IADP,IAEAA,GAAG,IAAI,EAFP,IAGAA,GAAG,IAAI,EAHP,IAIAA,GAAG,IAAI,WAJP,IAKAA,GAAG,IAAI,MALP,IAMAA,GAAG,IAAI,MAPT,EAQE;IACA,OAAO,IAAP;EACD;;EACD,OAAO,KAAP;AACD;AAED;AACA;AACA;AACA;AACA;AACA;;AACA,OAAO,SAASC,WAAT,CAAqBC,IAArB,EAA2BC,MAA3B,EAAmC;EACxCA,MAAM,GAAGA,MAAM,IAAIC,MAAM,CAACC,QAAP,CAAgBF,MAAhB,CAAuBL,MAAvB,CAA8B,CAA9B,CAAV,IAA8CM,MAAM,CAACC,QAAP,CAAgBC,IAAhB,CAAqBC,KAArB,CAA2B,GAA3B,EAAgC,CAAhC,CAAvD;;EACA,IAAI,CAACJ,MAAL,EAAa;IACX,OAAO,IAAP;EACD;;EACD,IAAIK,GAAG,GAAG,IAAIC,MAAJ,CAAW,UAAUP,IAAV,GAAiB,eAA5B,CAAV;EACA,IAAIQ,CAAC,GAAGP,MAAM,CAACQ,KAAP,CAAaH,GAAb,CAAR;EACA,IAAIE,CAAC,IAAI,IAAT,EAAe,OAAOE,QAAQ,CAACF,CAAC,CAAC,CAAD,CAAF,CAAf;EACf,OAAO,IAAP;AACD"}]}